{
  "compilerOptions": {
    "strict": true,
    "pretty": true,
    "noEmit": true,
    "sourceMap": true,
    "declaration": true,
    "importHelpers": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "module": "commonjs",
    "target": "es5"
  },
  "exclude": [
    "node_modules"
  ]
}